.urllist-with-thumbnails
  .urllist-item
    position: relative
    min-height: 170px

  .urllist-item-inner
    position: absolute
    z-index: 500000
    width: 100%
    height: 100%
    margin: auto

  .urllist-image
    position: absolute
    z-index: 499999
    width: 100%

  .urllist-title
    position: absolute
    z-index: 500002
    width: 100%
    height: 100%
    box-sizing: border-box
    padding: 15px 10px 0
    text-align: center
    text-rendering: optimizeLegibility
    font-size: 23px
    line-height: 25px
    text-overflow: ellipsis
    color: #fff !important
    background-color: rgba(0, 0, 0, 0.6)
    transition: background-color 1s ease-out

  .urllist-title:hover
    background-color: rgba(0, 0, 0, 0.2)

  .urllist-categories
    position: absolute
    z-index: 500003
    width: 100%
    padding: 0 5px
    box-sizing: border-box
    bottom: 0

  .urllist-item
    position: relative
    min-height: 170px
