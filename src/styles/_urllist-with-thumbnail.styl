.urllist-with-thumbnails
  .urllist-item
    position: relative
    min-height: 170px

  .urllist-item-inner
    position: absolute
    z-index: 0
    width: 100%
    height: 100%
    margin: auto

  .urllist-image
    position: absolute
    width: 100%

  .urllist-title
    position: absolute
    z-index: 500002
    width: 100%
    height: 100%
    box-sizing: border-box
    padding: 15px 10px 0
    text-align: center
    text-rendering: optimizeLegibility
    font-size: 23px
    line-height: 25px
    text-overflow: ellipsis
    color: #fff !important
    background: radial-gradient(rgba(6, 47, 76, 0.04), rgba(3, 20, 37, 0.8))

  .urllist-title:hover
    background: radial-gradient(rgba(6, 47, 76, 0.00), rgba(3, 20, 37, 0.5))

  .urllist-categories
    position: absolute
    z-index: 500003
    width: 100%
    padding: 0 5px
    box-sizing: border-box
    bottom: 0

  .urllist-item
    position: relative
    min-height: 170px
